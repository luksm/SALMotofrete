<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="SALClassLib.OS.Model" assembly="SALClassLib">
  <class name="FilaOrdemServico" table="TB_OS_FILA">
    <id name="Id">
      <column name="ID_OS_COBRANCA" not-null="true"/>
      <generator class="increment" />
    </id>
    <property name="Posicao" column="POSICAO" not-null="true" />
    <many-to-one name="OrdemServico" column="ID_ORDEM_SERVICO" cascade ="none" class="OrdemServico" not-null="true" unique="true" />
  </class>
</hibernate-mapping>