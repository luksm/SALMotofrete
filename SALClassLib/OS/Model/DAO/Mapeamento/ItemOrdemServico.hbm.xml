<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="SALClassLib.OS.Model" assembly="SALClassLib">
  <class name="ItemOrdemServico" table="TB_OS_ITEM">
    <id name="Id">
      <column name="ID_OS_ITEM" not-null="true"/>
      <generator class="increment" />
    </id>
    <property name="Descricao" column="DESCRICAO" not-null="true" />
    <property name="Peso" column="PESO" not-null="true" />
    <property name="Volume" column="VOLUME" not-null="true" />
  </class>
</hibernate-mapping>