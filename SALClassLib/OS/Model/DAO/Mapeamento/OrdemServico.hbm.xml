<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="SALClassLib.OS.Model" assembly="SALClassLib">
  <class name="OrdemServico" table="TB_ORDEM_SERVICO">
    <id name="Id">
      <column name="ID_ORDEM_SERVICO" not-null="true"/>
      <generator class="increment" />
    </id>
    <property name="Numero" column="NUMERO" not-null="true" />
    <property name="Data" column="DATA_CADASTRO" not-null="true" />
    <property name="Observacao" column="OBSERVACAO" not-null="false" />
    <many-to-one name="EnderecoRetirada" class="EnderecoRetirada" column="ID_ENDERECO_OS_RETIRADA" cascade="all" not-null="true" unique="true"  />
    <many-to-one name="EnderecoEntrega" class="EnderecoEntrega" column="ID_ENDERECO_OS_ENTREGA" cascade="all" not-null="true" unique="true"  />
    <many-to-one name="Cobranca" class="Cobranca" column="ID_COBRANCA" cascade="all" not-null="true" unique="true" />
    
  </class>
</hibernate-mapping>