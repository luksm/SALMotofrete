<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="SALClassLib.OS.Model" assembly="SALClassLib">
  <class name="OrdemServico" table="TB_ORDEM_SERVICO">
    <id name="Id">
      <column name="ID_ORDEM_SERVICO" not-null="true"/>
      <generator class="increment" />
    </id>
    <property name="Numero" column="NUMERO" not-null="true" />
    <property name="Data" column="DATA_CADASTRO" not-null="true" />
    <property name="Observacao" column="OBSERVACAO" not-null="false" />
    <set name="EnderecosRetirada" table="TB_ENDERECOS_OS_RETIRADA" cascade="all">
      <key column="ID_ORDEM_SERVICO" />
      <one-to-many class="EnderecoOrdemServico" />
        <!--<composite-element class="EnderecoOrdemServico">
        <property name="NomeContato" column="NOME_CONTATO" length="50" not-null="true" />
        <property name="TelefoneContato" column="TELEFONE_CONTATO" length="15" not-null="true" />
      </composite-element>-->
    </set>
    <set name="EnderecosEntrega" table="TB_ENDERECOS_OS_ENTREGA" cascade="all">
      <key column="ID_ORDEM_SERVICO" />
      <one-to-many class="EnderecoOrdemServico" />
      <!--<composite-element class="EnderecoOrdemServico">
        <property name="NomeContato" column="NOME_CONTATO" length="50" not-null="true" />
        <property name="TelefoneContato" column="TELEFONE_CONTATO" length="15" not-null="true" />
        <property name="Observacao" column="OBSERVACAO" not-null="false" />
      </composite-element>-->
    </set>
  </class>
</hibernate-mapping>