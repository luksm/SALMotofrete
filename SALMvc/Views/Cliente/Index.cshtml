@model IEnumerable<SALClassLib.Masterdata.Model.Cliente>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_Shared/utopia.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Nova Pessoa Fisica", "CreatePF")
</p>
<p>
    @Html.ActionLink("Nova Pessoa Juridica", "CreatePF")
</p>
<table>
    <tr>
        <th>Nome</th>
        <th>Sobrenome</th>
        <th>CPF</th>
        <th>RG</th>
        <th>Email</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
